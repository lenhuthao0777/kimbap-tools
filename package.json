{
  "name": "kimbap-tools",
  "private": false,
  "description": "Beautiful and accessible React UI library for building web3 applications",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "author": "Johnny.dev",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "turbo:clean": "turbo clean && rimraf ./node_modules/.cache/turbo ./packages/**/.turbo ./packages/**/**/.turbo ./apps/**/**/.turbo",
    "clean:node-modules": "rimraf ./apps/**/node_modules ./packages/**/node_modules ./packages/**/**/node_modules ./node_modules",
    "clean:dist": "rimraf ./packages/**/**/dist ./packages/**/dist ./apps/**/.next ./packages/**/storybook-static",
    "clean:lock": "rm ./pnpm-lock.yaml"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "@typescript-config-custom": "workspace:*",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "turbo": "latest",
    "@swc-node/jest": "^1.6.8",
    "@swc/core": "^1.3.105",
    "@swc/jest": "^0.2.31",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "^18.2.46",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "prettier-plugin-tailwindcss": "^0.5.11"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  }
}
